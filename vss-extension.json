{
    "manifestVersion": 1,
    "id": "this-is-a-test-123123",
    "version": "1.0.6",
    "name": "My first sample extension",
    "description": "A sample Visual Studio Services extension.",
    "publisher": "TobiasGlenk",
    "categories": ["Azure Boards"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
            }
        ],
        "contributions": [
            {
                "id": "Test Hub",
                "type": "ms.vss-web.hub",
                "targets": [
                    "ms.vss-work-web.work-hub-group"
                    ],
                "properties": {
                    "name": "Workitem Hub",
                    "order": 99,
                    "uri": "src/Create_New_Board_Hub/hello-world.html"
                }
            },
            {  
                "id": "sample-work-item-menu",  
                "type": "ms.vss-web.action",  
                "description": "Sample toolbar item which updates the title of a work item",  
                "targets": [  
                    "ms.vss-work-web.work-item-context-menu"  
                ],  
                "properties": {  
                    "text": "Try me!",  
                    "title": "Updates the title of the work item from the extension",  
                    "toolbarText": "Try me!",  
                    "icon": "images/hotdog.png",  
                    "uri": "src/Add_Menu_Action/change_Workitem_Title.html"  
                }  
            }
        ],
        "scopes": [
			"vso.work"
		],
    "icons": {
        "default": "images/extension_img.jfif"
     },
    "files": [
        {
            "path": "sdk/scripts", 
            "addressable": true
        },
        {
            "path": "images/extension_img.jfif", 
            "addressable": true
        },
        {
            "path": "src",
            "addressable": true
        }
    ]
}